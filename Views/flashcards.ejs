            { front: "Une réunion", back: "A meeting" },
            { front: "Un rendez-vous", back: "An appointment" },
            { front: "Un entretien", back: "An interview" },
            { front: "Un emploi", back: "A job" },
            { front: "Un stage", back: "An internship" },
            { front: "Un CV", back: "A resume" },
            { front: "Un salaire", back: "A salary" },
