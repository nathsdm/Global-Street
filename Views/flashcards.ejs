            { front: "Un bureau", back: "An office" },
            { front: "Un ordinateur", back: "A computer" },
            { front: "Un portable", back: "A laptop" },
            { front: "Un téléphone", back: "A phone" },
            { front: "Un stylo", back: "A pen" },
            { front: "Un crayon", back: "A pencil" },
            { front: "Un papier", back: "A paper" },
            { front: "Un dossier", back: "A file" },
            { front: "Un document", back: "A document" },
            { front: "Un contrat", back: "A contract" },
            { front: "Un stage", back: "An internship" },
            { front: "Un emploi", back: "A job" },
            { front: "Un salaire", back: "A salary" },
            { front: "Un employé", back: "An employee" },
            { front: "Un employeur", back: "An employer" },
            { front: "Un collègue", back: "A colleague" },
            { front: "Un bureau", back: "An office" },
            { front: "Un ordinateur", back: "A computer" },
            { front: "Un portable", back: "A laptop" },
            { front: "Un téléphone", back: "A phone" },
            { front: "Un stylo", back: "A pen" }
        ];
        var cardNum = 0;
        function previousCard() {
            if (cardNum == 0) {
                return;
            }
            cardNum--;
            var front = document.getElementById("front");
            var back = document.getElementById("back");
            var card = listOfCards[cardNum];
            front.innerHTML = card.front;
            back.innerHTML = card.back;
        }
        function nextCard() {
            if (cardNum == listOfCards.length - 1) {
                return;
            }
            cardNum++;
            var front = document.getElementById("front");
            var back = document.getElementById("back");
            var card = listOfCards[cardNum];
            front.innerHTML = card.front;
            back.innerHTML = card.back;
        }
    </script>

